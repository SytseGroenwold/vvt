woordenboek = '[{"woord": "aalmoes","beschrijving":"(v.; aalmoezen) [<Frans almosne <Latijn elemosina <Grieks eleèmosunè], 1 deernisgift."},{"woord": "aartsbisdom","beschrijving":"(o.; -men), 1 hoofdsticht [‘sticht’ is een verouderd woord voor bisdom, vergelijk de oude benamingen ‘Het Sticht’ en ‘Oversticht’ voor de gouwen Utrecht en Overijssel]."},{"woord": "aartsbisschop","beschrijving":"(m.; -pen), 1 hoofdstichtsvader."},{"woord": "ablativus","beschrijving":"(m.; ablativi) [Latijn (letterlijk weggebracht), een woord bedacht door Quintilianus (35-96)], 1 oorsprongsnaamval, zesde naamval."},{"woord": "ablaut","beschrijving":"(m.; -en) [Hoogduits, een woord bedacht door Jakob Ludwig Karl Grimm (1785-1863)], 1 klankwisseling, klinkerwisseling."},{"woord": "abonnee","beschrijving":"(m. / v.; -s) [<Frans abonné], 1 ingetekende."},{"woord": "abonnement","beschrijving":"(o.; -en) [Frans), 1 intekening."},{"woord": "abonneren","beschrijving":"(wederkerend of overgankelijk werkwoord) [<Frans abonner], 1 intekenen."},{"woord": "abortus","beschrijving":"(m.; -sen) [Latijn, van aboriri (vergaan, verloren gaan)], 1 ontijdige geboorte, na minder dan 16 weken zwangerschap  »  miskraam; 2 opzettelijk veroorzaakte afbreking van de zwangerschap  »  afdrijving, vazelafdrijving [van het verouderde woord vazel = foetus, ongeboren vrucht]."},{"woord": "abrupt","beschrijving":"(bn.) [Frans <Latijn abruptus, van ab = af + rumpere = breken], 1 kort afgebroken  »  hortend; 2 onsamenhangend; 3 plotseling."},{"woord": "absent","beschrijving":"(bn.) [(1544) <Frans (± 1185) <Latijn absens], 1 afwezig; 2 verstrooid."},{"woord": "absentie","beschrijving":"(v.; -s) [<Latijn absentia], 1 afwezigheid; 2 verstrooidheid."},{"woord": "absoluut","beschrijving":"[(1595) <Frans absolut <Latijn absolutus], I (bn.), 1 niet beschouwd met betrekking tot iets soortgelijks  »  volstrekt; 2 het genoemde ten volle zijnde  »  volkomen; 3 (van stoffen) rein, onvermengd; 4 geheel onafhankelijk  »  onbeperkt;"},{"woord": "II","beschrijving":"(bw van hoedanigheid), 1 volstrekt, geheel."},{"woord": "absorberen","beschrijving":"(overgankelijk werkwoord) [(1619) <Frans absorber (13de eeuw) <Latijn absorbere (verzwelgen, indrinken)], 1 opslorpen, indrinken (zowel letterlijk als figuurlijk)."},{"woord": "absorptie","beschrijving":"(v.; geen meervoud) [<Frans absorption <Latijn absorptio (opslorping)], 1 (scheikunde) verschijnsel dat een gas of vloeistof in het volume van een vaste stof of vloeistof wordt opgenomen en vastgehouden  »  opslorping, indrinking (voor vloeistoffen in een vaste stof), inzuiging (voor gassen in een vaste stof); 2 (verrichtingsleer) het opnemen (of: het opgenomen worden) van stoffen uit de omgeving door de cellen van een organisme  »  indrinking; 3 (natuurkunde) opname van de energie van een fysisch systeem door een ander systeem  »  opslorping."},{"woord": "abstract","beschrijving":"(bn.) [<Latijn abstractum = het afgetrokkene, van abstrahere = aftrekken; een Latijnse vertaling van het door Aristoteles gebezigde Griekse woord aphairesis = wegneming], 1 niet als vorm voorstelbaar  »  ontastbaar, onstoffelijk; 2 zich bezighoudend met afgeleide begrippen; (van kunsten) geen verband houdend met de zichtbare werkelijkheid  »  afgetrokken, aftogelijk [het laatste woord is een letterlijke vertaling in het Middelnederlands]."},{"woord": "absurd","beschrijving":"(bn.) [(1548) <Frans absurde of rechtstreeks <Latijn absurdus = onwelluidend, ongerijmd], 1 ongerijmd."},{"woord": "academie","beschrijving":"(v.; academies of academiën) [(1619) <Latijn Academia <Grieks Akadèmeia, de naam van een beroemd gymnasium in Athene, gesticht in de zesde eeuw v.C. en genoemd naar Academus], 1 vereniging ter beoefening en bevordering van wetenschap, letteren of kunst  »  genootschap; 2 hogeschool."},{"woord": "acanthus","beschrijving":"(m.; -sen) [Latijn <Grieks akantha (doorn)], 1 zuilbloem [de bladeren werden veel als versiering van Corinthische zuilen gebruikt]."},{"woord": "acceleratie","beschrijving":"(v.; -s) [<Frans accélération < Latijn acceleratio (het verhaasten) van celer = snel], 1 versnelling."},{"woord": "accelerator","beschrijving":"(m.; -s of -en) [Latijn (versneller)], 1 deeltjesversneller; 2 bepaalde pomp in de carburator van een automotor  »  optrekpomp; 3 middel om een scheikundige reactie te versnellen  »  werkingsversneller."},{"woord": "accelereren","beschrijving":"[<Frans accélérer], 1 (onovergankelijk werkwoord) optrekken; 2 (overgankelijk werkwoord) versnellen."},{"woord": "accent","beschrijving":"(o.; -en) [Frans <Latijn accentus of accantus van ad (bij) + cantus (gezang)], 1 nadruk of toonhoogte waardoor een klinker zich van andere onderscheidt  »  klemtoon; 2 nadruk; 3 eigenaardige wijze van uitspraak  »  tongval."},{"woord": "accentueren","beschrijving":"(overgankelijk werkwoord) [<Frans accentuer], 1 (in alle betekenissen) beklemtonen, benadrukken."},{"woord": "accentvers","beschrijving":"(o.; accentverzen), 1 heffingvers."},{"woord": "accept","beschrijving":"(o.; -en) [<Hoogduits Akzept <Latijn accipere (aannemen)], 1 verklaring waarbij een wissel geaccepteerd wordt  »  wisselaanvaarding."},{"woord": "acceptabel","beschrijving":"(bn.) [<Frans acceptable], 1 aannemelijk, aanvaardbaar."},{"woord": "acceptatie","beschrijving":"(o.; -s) [<Frans acceptation] , 1 aanvaarding."},{"woord": "accepteren","beschrijving":"(overgankelijk werkwoord) [<Frans accepter <Latijn accipere], 1 aannemen, aanvaarden."},{"woord": "acceptgirokaart","beschrijving":"(v.; -en), 1 overschrijvingskaart."},{"woord": "acceptor","beschrijving":"(m.; -s of -en) [Latijn, hij die iets aanneemt], 1 cel of orgaan die of dat een prikkel opneemt  »  prikkelopnemer; 2 atoom of molecule dat een extra elektron kan ontvangen  »  ontvangstdeeltje."},{"woord": "accessoire","beschrijving":"(o.; -s) [Frans], 1 bijkomstigheid, (in het meervoud) toebehoren."},{"woord": "accijns","beschrijving":"(m.; accijnzen) [(1526) onder invloed van cijns (belasting) gevormd uit accijs <Latijn accisia (belasting) van accidere (afsnijden)], 1 ongeld [Middelnederlands]."},{"woord": "acclimatisatie","beschrijving":"(v.; -s) [<Frans acclimatation], 1 luchtstreekgewenning."},{"woord": "acclimatiseren","beschrijving":"(onovergankelijk of wederkerend werkwoord) [<Frans acclimater], 1 luchtstreekgewennen."},{"woord": "accolade","beschrijving":"(v.; -s) [Frans (omhelzing), van Latijn collum (hals)], 1 omhelzingshaak."},{"woord": "accommodatie","beschrijving":"(v.; -s) [<Frans accomodation (het naar iets schikken)], 1 het zich schikken naar de omstandigheden  »  aanpassing; 2 al hetgeen voor het verblijf van personen is aangebracht of ingericht  »  verblijfsvoorziening."},{"woord": "accompagneren","beschrijving":"(overgankelijk werkwoord) [<Frans accompagner], 1 (in alle betekenissen) begeleiden."},{"woord": "accordeon","beschrijving":"(o. & m.; -s) [<Frans accordéon <Hoogduits Akkordion, gevormd in 1829 door Cyril Damian van Akkord (akkoord)], 1 balgharp."},{"woord": "accountancy","beschrijving":"(v.; geen meervoud) [Engels], 1 boekschouwing."},{"woord": "accountant","beschrijving":"(m.; -s) [Engels <Oudfrans accomptant (rekenmeester)], 1 boekschouwer."},{"woord": "accu","beschrijving":"(m.; -’s) [Frans (± 1930) verkort uit accumulateur], zie accumulator."},{"woord": "accumulatie","beschrijving":"(v.; -s) [<Frans accumulation <Latijn accumulatio], 1 opeenhoping."},{"woord": "accumulator","beschrijving":"(m.; -s of -en) [Latijn (ophoper)], 1 barnkrachtvat [van barnkracht of barnsteenkracht = elektriciteit]."},{"woord": "accuraat","beschrijving":"(bn.) [<Hoogduits Akkurat <Latijn accuratus], 1 nauwgezet."},{"woord": "accuratesse","beschrijving":"(v.; geen meervoud) [<Hoogduits Akkuratesse <Latijn accuratus], 1 nauwgezetheid."},{"woord": "accusativus","beschrijving":"(m.; accusativi) [Latijn, van causa (oorzaak), een vertaling van het Griekse aitiakè ptoosis (veroorzaakte naamval) van aitia = oorzaak], 1 vierde naamval."},{"woord": "ace","beschrijving":"(m.; -s) [Engels <Oudfrans ais <Latijn as], 1 aas."},{"woord": "aceton","beschrijving":"(o. & m.) [gevormd van Latijn acetum (azijn)], 1 azijngeest."},{"woord": "acidimeter","beschrijving":"(m.; -s) [<Frans acidimètre, gevormd van Latijn aciditas (zuurheid)], 1 zuurmeter."},{"woord": "acidimetrie","beschrijving":"(v.) [zie acidimeter], 1 zuurmeting."},{"woord": "aciditeit","beschrijving":"(v.; -en) [<Frans acidité <Latijn aciditas], 1 zuurheid."},{"woord": "acidofiel","beschrijving":"(bn.) [van Latijn acidum (zuur) en Grieks philos (beminnend)], 1 zuurminnend."},{"woord": "acne","beschrijving":"(v.; -s) [In omloop gebracht door een handschriftnaschrijver die in plaats van het Griekse akmè (puntje) aknè schreef], 1 vetpuist; 2 (in het bijzonder) talgklierverhoorning."},{"woord": "acognosie","beschrijving":"(v.) [gevormd van Grieks akos (geneesmiddel) en gnoosis (inzicht)], 1 geneesmiddelenleer, heelstleer [heelst is een mogelijke vertaling voor medicijn]."},{"woord": "aconitine","beschrijving":"(v.), 1 bepaald alkaloïde dat voorkomt in planten van het geslacht Aconitum, Monnikskap  »  monnikskapbitter [-bitter is het achtervoegsel dat gebruikt wordt voor alkaloïden]."},{"woord": "acquisitie","beschrijving":"(v.; -s) [<Latijn acquisitio], 1 verwerving, werving; 2 aanwinst."},{"woord": "acrobaat","beschrijving":"(m. ;acrobaten) [<Hoogduits Akrobat <Frans acrobate <Grieks akrobatos (op de tenen lopend)], 1 (oorspronkelijk) snoerdanser; 2 (tegenwoordig) kunstenmaker."},{"woord": "acrostichon","beschrijving":"(o.; -s) [<Grieks akrostichis, van akros (uitstekend) + stichos (rij, vers)], 1 naamvers, naamgedicht."},{"woord": "acteren","beschrijving":"(overgankelijk en onovergankelijk werkwoord) [van het Franse acte (toneelstuk in één bedrijf)], 1 schouwspelen, toneelspelen."},{"woord": "acteur","beschrijving":"(m.; -s), actrice (v.; -s) [(1687) <Frans acteur (13de eeuw)], schouwspeler, toneelspeler."},{"woord": "actie","beschrijving":"(v.; -s of actiën) [<Frans action of rechtstreeks <Latijn actio, van agere (handelen)], 1 (in het algemeen) werking, handeling ; - (in het bijzonder) rechtshandeling, rechtsvordering; 2 beweging, bedrijvigheid, (in het bijzonder) in actie komen  »  in het geweer komen, ter daad komen; 3 gezamenlijk opgezet en tot uiting gebracht streven  »  daadslag; (in het bijzonder) staking; 4 aandeel in een onderneming  »  aandeel."},{"woord": "actief","beschrijving":"(bn.) [<Frans actif], 1 (in het algemeen) werkzaam, dadig; 2 in dienst  »  dienstdoend; 3 metterdaad; 4 (van een geldsom) nog te goed  »  uitstaand; 5 (taalkunde) bedrijvend."},{"woord": "activeren","beschrijving":"(overgankelijk werkwoord) [<Frans activer], 1 actief maken (in het algemeen)  »  verwerkzamen; (in het bijzonder met betrekking tot een scheikundige stof) aanwakkeren; 2 tot actie aanmanen  »  aansporen."},{"woord": "activiteit","beschrijving":"(v.; -en) [<Frans activité], 1 (in het algemeen) werkzaamheid, dadigheid; - (in het bijzonder) voortvarende werkzaamheid  »  bedrijvigheid; 2 het in dienst zijn  »  dienstdoendheid; 3 radioactiviteit  »  straalwerkzaamheid."},{"woord": "actualiseren","beschrijving":"(overgankelijk werkwoord) [<Frans actualiser], 1 verwerkelijken; 2 aan de eigen tijd aanpassen  »  verhedendaagsen."},{"woord": "actualiteit","beschrijving":"(v.; --en) [<Frans actualité], 1 bestaande toestand  »  werkelijkheid; 2 gebeurtenis van het ogenblik of het aan de orde zijn  »  hedendaagsheid, huidigheid, hedendadigheid."},{"woord": "actueel","beschrijving":"(bn.) [<Frans actuel], 1 huidig, hedendaags, hedendadig."},{"woord": "acupunctuur","beschrijving":"(v.; acupuncturen) [gevormd van het Latijnse acus (naald) + punctura (het prikken)], 1 naaldprikgeneeswijze, naaldprikgenezing."},{"woord": "acuut","beschrijving":"(bn.) [<Latijn acutus (scherp, snijdend)], 1 (van ziekten) schielijk; 2 (van kwesties) dringend."},{"woord": "adapter","beschrijving":"(m.; -s) [Engels <Latijn adaptere (aanpassen)], 1 apparaat om tussen een stopcontact en een steker te plaatsen bij onderling afwijkende systemen  »  verloopstuk, verloopsteker."},{"woord": "adequaat","beschrijving":"(bn.) [<Latijn adaequatus, van adaequare (gelijk maken)], 1 overeenstemmend met het voorbeeld  »  gelijkwaardig; 2 geschikt voor het beoogde doel  »  gadelijk [het laatste is een verouderd woord met de betekenis „geschikt, wat aard en bestemming betreft, en vandaar gemakkelijk, dienstig voor het gebruik waarvoor de zaak bestemd is” (WNT)]."},{"woord": "adhesie","beschrijving":"(v.; geen meervoud) [<Latijn adhaesio (aankleving)], 1 aankleving, aanklevingskracht."},{"woord": "adjectivum","beschrijving":"(o.; adjectiva) [<Latijn adiectivus, een vertalende ontlening aan het Griekse epitheton], 1 bijvoeglijk naamwoord, eigenschapswoord."},{"woord": "administrateur","beschrijving":"(m.; -s) [Frans], 1 onderwinder [zie administratie]."},{"woord": "administratie","beschrijving":"(v.; -s) [<Frans administration <Latijn administratio (beheer)], 1 onderwind (o.; -en) [een verouderd woord, afgeleid van onderwinden (zich met iets bemoeien, later: beheren, besturen, administreren); staat naast bewind]."},{"woord": "administratief","beschrijving":"(bn.) [<Frans administratif], 1 bestuurlijk, onderwindelijk [zie administratie]."},{"woord": "administreren","beschrijving":"(overgankelijk werkwoord) [<Frans administrer], 1 beheren, besturen, onderwinden [het laatste is een verouderd woord (zich met iets bemoeien, later: beheren, besturen, administreren); staat naast het nog gangbare woord bewind van het werkwoord bewinden]."},{"woord": "admiraal","beschrijving":"(m.; -s of admiralen) [Vroeger (1337-) ammiral <Arabisch amir (bevelhebber)], 1 vlootvorst; 2 vlootvorstvlinder; 3 vlaggeschip."},{"woord": "adolescent","beschrijving":"(m.; -en), adolescente (v.; -s) [Frans <Latijn adolescens (jonge man), van adolescere (opgroeien, volwassen worden), van olescere (groeien)], 1 jongeling."},{"woord": "adolescentie","beschrijving":"(v.; geen meervoud) [<Latijn adolescentia], 1 jongelingschap."},{"woord": "adopteren","beschrijving":"(overgankelijk werkwoord) [<Frans adopter <Latijn adoptare (uitkiezen, opnemen in de familie)], 1 ingezinnen."},{"woord": "adoptie","beschrijving":"(v.; -s) [<Frans adoption <Latijn adoptio], 1 ingezinning."},{"woord": "ad","beschrijving":"rem (bijwoordelijke verbinding) [Latijn, van ad (bij) + res (zaak)], 1 ter zake."},{"woord": "adrem","beschrijving":"(bn.) [<Latijn ad rem], 1 gevat, snedig."},{"woord": "adrenaline","beschrijving":"(v.) [van het Latijnse ad (bij) + renes (nieren)], 1 bijnierwekstof [wekstof = hormoon]."},{"woord": "adres","beschrijving":"(o.; -sen) [(1783) <Frans adresse, van adresser; dresser <volkslatijn directiare (richten). De betekenis ‘verzoekschrift’ uit het Engels.], 1 opgave van woon- of verblijfplaats; opschrift op een brief  »  woonmerk, toeschrift; 2 verzoekschrift."},{"woord": "adresseren","beschrijving":"[<Frans adresser, zie adres], (overgankelijk werkwoord) 1 een adres zetten op…  »  woonmerken, betoeschriften; 2 richten tot, toeschrijven aan; (onovergankelijk werkwoord) 3 een verzoekschrift indienen  »  verzoekschrijven."},{"woord": "advent","beschrijving":"(m.; -en) [van het Latijn adventus Domini (aankomst des Heren)], 1 voorhoogtijd [reeds bestaand woord, van hoogtijd ((kerkelijk) feest)]."},{"woord": "adventisme","beschrijving":"(o.) [<Engels adventism, van second advent (wederkomst, van Christus)], 1 wederkomstgeloof."},{"woord": "adventist","beschrijving":"(m.; -en) [Engels, van second advent (wederkomst, van Christus)], 1 wederkomstgelover."},{"woord": "advertentie","beschrijving":"(v.; -s) [<Frans advertance <Latijn advertentia (aandacht)], 1 aankondiging, kondmaking [van kond = bekend]."},{"woord": "advies","beschrijving":"(o.; adviezen) [Verlatijnst uit Middelnederlands avijs <Frans avis (raad)], 1 raadgeving, aanrading."},{"woord": "adviseren","beschrijving":"(overgankelijk werkwoord), 1 aanraden, raadgeven."},{"woord": "adviseur","beschrijving":